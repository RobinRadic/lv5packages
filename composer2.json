{
    "name": "laradic/workspace",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl"
        }
    ],
    "prefer-stable": true,
    "require": {
        "laravel/framework": "5.0.*",
        "gooh/interface-distiller": "dev-master",
        "illuminate/html": "~5.0",
        "laravel/laravel": "5.0.*",
        "filp/whoops": "~1.1",
        "barryvdh/laravel-ide-helper": "2.*",
        "barryvdh/laravel-debugbar": "~2.0",
        "raveren/kint": "0.9.*",
        "orchestra/testbench": "3.0.*",
        "erusev/parsedown": "~1.5",
        "kriswallsmith/assetic": "~1.2",
        "leafo/lessphp": "0.5.*",
        "leafo/scssphp": "0.1.*",
        "ptachoire/cssembed": "~1.0",
        "kzykhys/ciconia": "~1.0",
        "mockery/mockery": "0.9.*",
        "composer/composer": "~1.0@dev",
        "composer/installers": "~1.0@dev",
        "athletic/athletic": "~0.1",
        "tracy/tracy": "2.3.*",
        "way/generators": "~3.0",
        "symfony/yaml": "~2.6",
        "webmozart/path-util": "~1.0",
        "ccampbell/chromephp": "~4.1",
        "firephp/firephp-core": "0.4.*",
        "naneau/semver": "0.0.7.*",
        "radic/tmp-underscore-php": "dev-master",
        "soapbox/laravel-formatter": "~2.0",
        "orchestra/foundation": "3.0.*",
        "theseer/phpdox": "0.7.*",
        "knplabs/github-api": "~1.4",
        "graham-campbell/github": "~2.0",
        "symfony/dom-crawler": "~2.6",
        "pimple/pimple": "1.0.*",
        "herrera-io/phar-update": "2.0.0",
        "apigen/apigen": "~4.0",
        "latte/latte": "~2.3",
        "laravelcollective/html": "~5.0",
        "graham-campbell/throttle": "~3.0",
        "clickalicious/phpmemadmin": "0.3.*",
        "itsgoingd/clockwork": "dev-master",
        "orchestra/platform": "~3.0",
        "cartalyst/sentry": "dev-feature/laravel-5",
        "cartalyst/alerts": "~2.0",
        "cartalyst/tags": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database",

            "laradic/console/src/controllers"
        ],
        "psr-0": {
            "Darsain\\Console\\": "laradic/console/src/"
        },
        "psr-4": {
            "App\\": "app/",
            "Radic\\BladeExtensions\\": "laradic/blade-extensions/src/",
            "Laradic\\Config\\": "laradic/config/src/",
            "Laradic\\Debug\\": "laradic/debug/src/",
            "Laradic\\Themes\\": "laradic/themes/src/",
            "Laradic\\Support\\": "laradic/support/src/",
            "Laradic\\Docs\\": "laradic/docs/src/",
            "Laradic\\Admin\\": "laradic/admin/src/",
            "Laradic\\Dev\\": "laradic/dev/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Radic\\Tests\\BladeExtensions\\": "laradic/blade-extensions/tests/",
            "Laradic\\Tests\\Dev\\": "laradic/dev/tests/",
            "Laradic\\Tests\\Themes\\": "laradic/themes/tests/",
            "Laradic\\Tests\\Support\\": "laradic/support/tests/",
            "Laradic\\Tests\\Docs\\": "laradic/docs/tests/",
            "Laradic\\Tests\\Debug\\": "laradic/debug/tests/",
            "Laradic\\Tests\\Admin\\": "laradic/admin/tests/",
            "Laradic\\Tests\\Config\\": "laradic/config/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [

            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "php artisan ide-helper:generate"
        ]
    }
}
